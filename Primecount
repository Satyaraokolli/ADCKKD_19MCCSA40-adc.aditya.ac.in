import java.util.*;
class PrimeCount
{
public static void main(String args[])
{
  Scanner sc=new Scanner(System.in);
  System.out.println("enter a number");
  int n=sc.nextInt();
  int i,j;
  int count;
  int c=0;
  for(i=1;i<n;i++)
  {
    count=0;
    for(j=1;j<=i;j++)
    {
      if(i%j==0)
      count++;
    }
    if(count==2)
    c++;
  }
System.out.println("prime count="+c);
}
}

Output:
enter a number
10
prime count=4  
  
