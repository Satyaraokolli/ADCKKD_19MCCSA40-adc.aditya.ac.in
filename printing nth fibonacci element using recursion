import java.util.*;

class Fibonacci
{  
  public static void main(String args[])
  {
      int n;
      Scanner sc=new Scanner(System.in);
      System.out.println("enter a nth place of fibonacci value");
      n=sc.nextInt();
      System.out.println(fib(n));  
  }
  public static int fib(int n)
  {
      if(n==0)
          return 0;
     else if(n==1)
          return 1;
      return fib(n-1)+fib(n-2);   
  }
}

Output:
enter a nth place of fibonacci value
6
8

